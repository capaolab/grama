datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id         Int      @id @default(autoincrement())
    email      String   @unique
    first_name String?
    last_name  String?
    birth_date DateTime
    created_at DateTime @default(now())
}
