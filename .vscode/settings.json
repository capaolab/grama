{
    "todo-tree.regex.regex": "(//)\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "todo-tree.ripgrep.ripgrepArgs": "--no-ignore",
    "todo-tree.tree.autoRefresh": true,
    "todo-tree.general.tags": [
        "TODO",
        "BUG",
        "REVIEW",
        "WARNING",
        "TEST"
    ],
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "icon": "bug",
            "background": "#ff000d",
            "foreground": "#880007",
        },
        "WARNING": {
            "icon": "alert",
            "background": "#00ffff",
            "foreground": "#028585",
        },
        "REVIEW": {
            "icon": "eye",
            "background": "#a788fd",
            "foreground": "#230181",
        },
        "TEST": {
            "icon": "tools",
            "background": "#ffd900",
            "foreground": "#998200",
        }
    }
}